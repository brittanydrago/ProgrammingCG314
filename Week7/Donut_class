//BrittanyDrago_class
float x;
float y;
float size;
color theColor;

//Constructor
Donut(float tempX, float tempY, float tempSize, color tempColor) {
  x = tempX;
  y = tempY;
  size = tempSize;
  theColor = tempColor;
}

void move() {
  x+=2
    if (x>width+size) {
    x=-size;
  }
}

void hover() {
  x+=random(-1, 1);
  y+=random(-1, 1);
  }
  
  void display()  {
    noStroke();
    fill(theColor);
   
  }
