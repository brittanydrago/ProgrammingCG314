void setup() {
  size (640, 480);
  background(0, 150, 60);
}

void draw() {
  float mappedValue = map(mouseX, 1, width, 1, 20);
  rect(mouseX, mouseY, mappedValue, mappedValue);

  fill(0, 175, 45);
  rect(10, 10, 60, 60);
  if (mouseX >10 && mouseX < 70 && mouseY > 10 && mouseY < 70) {
    fill(0, 125, 75);
    rect(10, 10, 60, 60);
  }
}
